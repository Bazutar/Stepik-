
int main() {
	int n = 0;

	while (n <= 100) {
		n++;
		if (n > 100) {
			break;
		}
		else {
			if (n % 3 == 0) {
				if (n % 5 == 0) {
					printf("FizzBuzz ");
				}
				else {
					printf("Fizz ");
				}
			}
			else {
				if (n % 5 == 0) {
					printf("Buzz ");
				}
				else {
					printf("%d ", n);
				}
			}
		}
	}
	return 0;
}
